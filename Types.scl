      nLaufWgNrNumber : DINT; 
      nProgrNumber : DINT; // 0 = free/unused; 9999 = wrong article
      nXbreiteRechts : DINT;
      nXbreitelinks : DINT;
      nYlaenge       : DINT;
      nTolX          : DINT; // optionale Toleranz X f端r diesen Artikel (abs)
      nTolY          : DINT; // optionale Toleranz Y f端r diesen Artikel (abs)
      nRelTolX       : DINT; // optionale relative Toleranz X in Prozent (0=unused)
      nRelTolY       : DINT; // optionale relative Toleranz Y in Prozent (0=unused)
      bIsNewArticle  : BOOL;
      bDataValid     : BOOL;
      nActXbreiteRechts : DINT;
      nActXbreitelinks : DINT;
      nActYlaenge       : DINT;
   END_STRUCT;
END_TYPE
